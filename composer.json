{
    "name": "ymlluo/group-robot",
    "description": "钉钉、企业微信、飞书 自定义群组机器人。支持链式语法创建发送消息。支持文本、Markdown、图片、文件、图文、卡片等消息。",
    "license": "MIT",
    "authors": [
        {
            "name": "Ymlluo",
            "email": "ymlluo@gmail.com",
            "homepage": "https://github.com/ymlluo"
        }
    ],
    "homepage": "https://github.com/ymlluo/group-robot",
    "keywords": ["Laravel", "GroupRobot","robot","dingtalk","wechat","feishu"],
    "require": {
        "php": "^7.1|^8.0",
        "ext-zip": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ymlluo\\GroupRobot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ymlluo\\GroupRobot\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ymlluo\\GroupRobot\\GroupRobotServiceProvider"
            ],
            "aliases": {
                "GroupRobot": "Ymlluo\\GroupRobot\\Facades\\GroupRobot"
            }
        }
    }
}
